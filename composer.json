{
    "name": "naykel/formit",
    "description": "Form components package NayKel Laravel applications",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Watts",
            "email": "nathan@naykel.com.au",
            "homepage": "https://naykel.com.au"
        }
    ],
    "keywords": ["Laravel", "Formit"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Naykel\\Formit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naykel\\Formit\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naykel\\Formit\\FormitServiceProvider"
            ]
        }
    }
}
